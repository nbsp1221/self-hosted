# Pin or track version:
# - `release` follows the latest stable release.
# - Or pin a version like `v0.30.0` to control upgrades.
KARAKEEP_VERSION=release

# Public URL of this instance
NEXTAUTH_URL=https://karakeep.example.com

# Secret for auth or session signing
# Generate: openssl rand -base64 36
NEXTAUTH_SECRET=change_me

# Meilisearch master key (alphanumeric)
# Generate: openssl rand -base64 48 | tr -dc 'A-Za-z0-9' | head -c 48; echo
MEILI_MASTER_KEY=change_me

# Runtime settings
LOG_LEVEL=notice
DB_WAL_MODE=true
TZ=Asia/Seoul

# Signup controls
DISABLE_SIGNUPS=false

# AI provider
# OPENAI_API_KEY=change_me
# OPENAI_BASE_URL=https://api.openai.com/v1

# AI inference settings
# INFERENCE_TEXT_MODEL=gpt-4.1-mini
# INFERENCE_IMAGE_MODEL=gpt-4o-mini
# INFERENCE_LANG=english
# INFERENCE_ENABLE_AUTO_TAGGING=true
# INFERENCE_ENABLE_AUTO_SUMMARIZATION=false
